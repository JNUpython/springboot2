<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssm.demo.dao.CustomerMapper">

    <insert id="saveCustomer" parameterType="com.ssm.demo.entity.Customer">
        INSERT INTO test.t_customer(`NAME`, gender, telephone, address)
        VALUES(#{name}, #{gender}, #{telephone}, #{address});
    </insert>

</mapper>